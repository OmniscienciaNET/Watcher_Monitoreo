@{
    ViewBag.Title = "Control de Áreas";
}
<link href="~/Content/Popup.css" rel="stylesheet" />
<table border="0" cellpadding="0" cellspacing="0" style="width:100%;">
    <tr>
        <td>
            <table align="center" border="0" cellpadding="0" cellspacing="0" class="context_table" style="width:99%;">
                <tr>
                    <td align="center" class="cab_context">OPCIONES DE BÚSQUEDA</td>
                </tr>
                <tr>
                    <td>
                        <table style="width:100%">
                            <tr>
                                <td style="width:32%;">
                                    <fieldset style="height:75px">
                                        <table>
                                            <tr>
                                                <td>Distribuidor</td>
                                                <td>
                                                    <input id="txtDstbCod" style="width:25px;text-transform:uppercase" maxlength="2" tabindex="1">
                                                    <input id="txtDstbDesc" style="width:240px" readonly="readonly">
                                                </td>
                                                <td style="padding-left:5px">
                                                    <button id="LookForAbnd" type="button" data-toggle="modal" data-target="#myModal" title="Buscar Abonado"><img alt="" src="~/Images/search.png" /></button>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>Abonado</td>
                                                <td>
                                                    <input id="txtAbndCod" style="width:60px;text-transform:uppercase" maxlength="10" tabindex="2">
                                                    <input id="txtAbndDesc" style="width:240px" readonly="readonly">
                                                </td>
                                                <td style="padding-left:5px">
                                                    <button id="btnClean" type="button" title="Limpiar"><img alt="" src="~/Images/Limpiar.png"></button>
                                                </td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </td>
                                <td style="width:15%">
                                    <fieldset style="height: 86px; margin-bottom: 12px">
                                        <legend style="font-size: 11px; margin-bottom: 0px; border-width: 0px; border-style: none; width: auto">
                                            Condición
                                        </legend>
                                        <table style="width:100%;margin-top:5px">
                                            <tr>
                                                <td>
                                                    @Html.TextBox("CondPrimCod", "", new { id = "txtCondicPrimCod", style = "width:30px;text-transform:uppercase;font-size:11px", maxlenght = 3, tabindex = 3 })
                                                    @Html.TextBox("CondPrimDes", "", new { id = "txtCondicPrimDes", @readonly = "readonly", style = "width:130px;font-size:11px" })
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    @Html.TextBox("CondUltCod", "", new { id = "txtCondicUltCod", style = "width:30px;text-transform:uppercase;font-size:11px", maxlenght = 3, tabindex = 4 })
                                                    @Html.TextBox("CondUltDes", "", new { id = "txtCondicUltDes", @readonly = "readonly", style = "width:130px;font-size:11px" })
                                                </td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </td>
                                <td style="width:26%;">
                                    <fieldset style="height:75px">
                                        <table>
                                            <tr>
                                                <td>Zona</td>
                                                <td colspan="3">Estado</td>
                                            </tr>
                                            <tr>
                                                <td style="padding-right:10px">
                                                    <input id="txtZona" type="text" tabindex="5" style="width: 80px;text-transform:uppercase" />
                                                </td>
                                                <td colspan="2">
                                                    <input id="rdbAll" type="radio" checked="checked" value="T" />Todos
                                                </td>
                                            </tr>
                                            <tr style="vertical-align:sub">
                                                <td>
                                                    <input id="chkDatosP" type="checkbox"/>Solo días pasados
                                                </td>
                                                <td>
                                                    <input id="rdbPlus" type="radio" value="E" />Exceso de Tiempo
                                                <td>
                                                    <input id="txtTime" type="text" disabled="disabled" style="width: 40px; margin-right: 10px; margin-left: 10px" value="0" />Minuto(s)
                                                </td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </td>
                                <td>
                                    <fieldset style="height:75px">
                                        <table style="margin-top:5px">
                                            <tr>
                                                <td style="padding-right:5px;padding-bottom:5px">Ubicación</td>
                                                <td style="padding-bottom:5px"><select id="ddlInOutSide" name="InOutSide"><option value="=">=</option><option value="≠">≠</option></select></td>
                                                <td style="padding-right:15px;padding-bottom:5px"><select id="ddlDpto" name="Ubigeo"><option selected="selected" value='00'>TODOS</option><option value='01'> AMAZONAS</option><option value='02'> ÁNCASH</option><option value='03'> APURÍMAC</option><option value='04'> AREQUIPA</option><option value='05'> AYACUCHO</option><option value='06'> CAJAMARCA</option><option value='07'> CALLAO</option><option value='08'> CUSCO</option><option value='09'> HUANCAVELICA</option><option value='10'> HUÁNUCO</option><option value='11'> ICA</option><option value='12'> JUNÍN</option><option value='13'> LA LIBERTAD</option><option value='14'> LAMBAYEQUE</option><option value='15'> LIMA</option><option value='16'> LORETO</option><option value='17'> MADRE DE DIOS</option><option value='18'> MOQUEGUA</option><option value='19'> PASCO</option><option value='20'> PIURA</option><option value='21'> PUNO</option><option value='22'> SAN MARTÍN</option><option value='23'> TACNA</option><option value='24'> TUMBES</option><option value='25'> UCAYALI</option></select></td>
                                                <td rowspan="2">
                                                    <button id="btnPlay" type="button" title="Play"><img id="imgbtnPlay" alt="" src="~/Images/start.png" width="16" height="16" /></button>
                                                    <button id="btnStop" type="button" title="Stop" disabled="disabled"><img id="imgbtnStop" alt="" src="~/Images/stop_d.png" width="16" height="16" /></button>
                                                    <button name="Buscar" id="btnSearch" title="Buscar" value=""><img src="~/Images/binocular.png" alt="" /></button>
                                                    <button id="btnReport" type="button" title="Reporte"><img id="imgbtnReport" alt="" src="~/Images/print.png" width="16" height="16" /></button><a id="exportarExcel" href="#" style="display:none"></a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="padding-right:5px">Tipo Abonado</td>
                                                <td colspan="2"><select id="ddlTipoAbonado" name="TipoAbonado"><option value='0'>TODOS</option><option value='1'>AGENCIA</option><option value='8'>AGENCIA ADM</option><option value='2'>ATM</option><option value='7'>ATM-N</option><option value='3'>BÓVEDA</option><option value='9'>COFRE</option><option value='6'>NO APLICA</option></select></td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    <tr>
        <td>
            <table align="center" border="0" cellpadding="0" cellspacing="0" class="context_table" style="width:99%">
                <tr>
                    <td style="width:100%;" valign="top" align="center">
                        <table style="width: 100%; font-size: x-small;border:none;border-collapse:collapse">
                            <tr style="background-color: darkgray; font-size: 9pt;font-weight:bold;height:30px">
                                <td style="width:20px;text-align:center">.</td>
                                <td style="width:70px">Fecha</td>
                                <td style="width:70px">Hora</td>
                                <td style="width:70px">Abonado</td>
                                <td style="width:280px;text-align:left">Oficina</td>
                                <td style="width:350px;text-align:left">Condición</td>
                                <td style="width:70px">ID Señal</td>
                                <td>Zona / Usuario / Descripción</td>
                            </tr>
                        </table>
                        <div id="divArea" style="height:250px; overflow:auto;">
                            <table style="width: 100%; font-size: x-small;border:none;border-collapse:collapse">
                                <tbody id="tbRegistrosArea"></tbody>
                            </table>
                        </div>
                        <table style="width:100%">
                            <tr>
                                <td style="text-align:center">
                                    <div id="divPaginacionArea" style="display:none">
                                        <nav style="text-align:center">
                                            <ul class="paginacion">
                                                <li id="liPaginaArea"></li>
                                            </ul>
                                        </nav>
                                    </div>
                                </td>
                            </tr>
                        </table>
                        <br />
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
@Html.Partial("BusquedaAbonado")
<div class="modal fade" id="myUserLogIn" tabindex="-1" role="dialog" aria-labelledby="myUserLogInLabel" style="width:40%; left:30%; outline:0">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background: linear-gradient(#04ACEC, #0186BA)">
                <button type="button" style="color:white" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myUserLogInLabel" style="font-size:14px;color:white;font-weight:bold">ADMINISTRADORES Y SUPERVISORES EN LÍNEA</h4>
            </div>
            <div class="modal-body">
                <div>
                    <table>
                        <tr>
                            <td>
                                <table style="width: 100%; font-size: x-small;border:none;border-collapse:collapse">
                                    <tr style="background-color: darkgray; font-size: 9pt;font-weight:bold;height:30px">
                                        <td style="text-align: left">NOMBRES Y APELLIDOS </td>
                                        <td style="width: 200px; text-align: left">CARGO</td>
                                    </tr>
                                </table>
                                <div style="height:150px; overflow:auto;">
                                    <table id="tbUserLogIn" style="width: 100%; font-size: x-small;border:none;border-collapse:collapse"></table>
                                </div>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrapPopup.js" type="text/javascript" async></script>
<script src="~/Scripts/ControlArea.js"></script>