@{
    ViewBag.Title = "Visor de Señales";
}
<link href="~/Content/Popup.css" rel="stylesheet" />
<table border="0" cellpadding="0" cellspacing="0" style="width:100%;">
    <tr>
        <td>
            <table align="center" border="0" cellpadding="0" cellspacing="0" class="context_table" style="width:99%;">
                <tr>
                    <td align="center" class="cab_context">OPCIONES DE BÚSQUEDA</td>
                </tr>
                <tr>
                    <td>
                        <table style="width:100%">
                            <tr>
                                <td style="width:60%;">
                                    <fieldset style="height:65px;">
                                        <table style="padding-top:3px">
                                            <tr>
                                                <td>Distribuidor</td>
                                                <td style="width:45px;">
                                                    <input id="txtDstbCod" style="width:40px;text-transform:uppercase" maxlength="2" tabindex="1">
                                                </td>
                                                <td style="padding-right:15px">
                                                    <input id="txtDstbDesc" style="width:240px" readonly="readonly">
                                                </td>
                                                <td>Abonado</td>
                                                <td style="width:45px;">
                                                    <input id="txtAbndCod" style="width:60px;text-transform:uppercase" maxlength="10" tabindex="2">
                                                </td>
                                                <td>
                                                    <input id="txtAbndDesc" style="width:240px" readonly="readonly">
                                                </td>
                                                <td style="padding-left:5px">
                                                    <table>
                                                        <tr>
                                                            <td>
                                                                <button id="LookForAbnd" type="button" data-toggle="modal" data-target="#myModal" title="Buscar Abonado"><img alt="" src="~/Images/search.png" /></button>
                                                            </td>
                                                            <td>
                                                                <button id="btnClean" type="button" title="Limpiar"><img alt="" src="~/Images/Limpiar.png"></button>        
                                                            </td>
                                                            

                                                        </tr>
                                                    </table>
                                                    
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Local
                                                </td>                                                
                                                <td style="width:45px;">
                                                    <input id="txtLocalid" style="width:60px;text-transform:uppercase" maxlength="10" tabindex="2">
                                                </td>
                                                <td>
                                                    <input id="txtLocalDesc" style="width:240px" readonly="readonly">
                                                </td>
                                                <td>
                                                    <button id="LookForLocal" type="button" data-toggle="modal" data-target="#myModalLocalSubscriber" title="Buscar Local"><img alt="" src="~/Images/search.png" /></button>
                                                </td>                                                
                                                <td>
                                                    Tipo de Partición
                                                </td>
                                                <td colspan="2">
                                                    <select id="ddlParticion" style="width:200px;font-size:10px;height:20px"></select>
                                                </td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </td>
                                <td>
                                    <fieldset style="height:65px;width:450px">
                                        <table style="margin-top:5px">
                                            <tr>
                                                <td style="padding-right:5px">Ubicación</td>
                                                <td><select id="ddlInOutSide"><option value="=">=</option><option value="≠">≠</option></select></td>
                                                <td style="padding-right:15px"><select id="ddlDpto"><option selected="selected" value='00'>TODOS</option><option value='01'> AMAZONAS</option><option value='02'> ÁNCASH</option><option value='03'> APURÍMAC</option><option value='04'> AREQUIPA</option><option value='05'> AYACUCHO</option><option value='06'> CAJAMARCA</option><option value='07'> CALLAO</option><option value='08'> CUSCO</option><option value='09'> HUANCAVELICA</option><option value='10'> HUÁNUCO</option><option value='11'> ICA</option><option value='12'> JUNÍN</option><option value='13'> LA LIBERTAD</option><option value='14'> LAMBAYEQUE</option><option value='15'> LIMA</option><option value='16'> LORETO</option><option value='17'> MADRE DE DIOS</option><option value='18'> MOQUEGUA</option><option value='19'> PASCO</option><option value='20'> PIURA</option><option value='21'> PUNO</option><option value='22'> SAN MARTÍN</option><option value='23'> TACNA</option><option value='24'> TUMBES</option><option value='25'> UCAYALI</option></select></td>
                                                <td>
                                                    <button id="btnPlay" type="button" title="Play"><img id="imgbtnPlay" alt="" src="~/Images/start.png" width="16" height="16" /></button>
                                                    <button id="btnStop" type="button" title="Stop" disabled="disabled"><img id="imgbtnStop" alt="" src="~/Images/stop_d.png" width="16" height="16" /></button>
                                                    <button id="btnSearch" title="Buscar" value=""><img src="~/Images/binocular.png" alt="" /></button>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="padding-right:5px">Tipo Abonado</td>
                                                <td colspan="2"><select id="ddlTipoAbonado"><option value='0'>TODOS</option><option value='1'>AGENCIA</option><option value='8'>AGENCIA ADM</option><option value='2'>ATM</option><option value='7'>ATM-N</option><option value='3'>BÓVEDA</option><option value='9'>COFRE</option><option value='6'>NO APLICA</option></select></td>
                                                <td>
                                                    <input id="chkEvitar" type="checkbox" style="vertical-align:sub">Evitar condiciones de autotest
                                                </td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    <tr>
        <td>
            <table align="center" border="0" cellpadding="0" cellspacing="0" class="context_table" style="width:99%">
                <tr>
                    <td style="width:100%;" valign="top" align="center">
                        <table style="width: 100%; font-size: x-small;border:none;border-collapse:collapse">
                            <tr style="background-color: darkgray; font-size: 9pt;font-weight:bold;height:30px">
                                <td style="width:20px;text-align:center">.</td>
                                <td style="width:60px">Fecha</td>
                                <td style="width:50px">Hora</td>
                                <td style="width:70px">Abonado</td>
                                <td style="width:280px;text-align:left">Oficina</td>
                                <td style="width:70px">ID Señal</td>
                                <td style="width:50px">Zona</td>
                                <td style="width:190px">Descripción</td>
                                <td style="width:160px">Descripción Zona</td>
                                <td style="width:40px">Área</td>
                                <td>Descripción Área</td>
                            </tr>
                        </table>
                        <div id="divVisor" style="height:350px; overflow:auto;">
                            <table style="width: 100%; font-size: x-small;border:none;border-collapse:collapse">
                                <tbody id="tbRegistrosVisor"></tbody>
                            </table>
                            <table style="width:100%">
                                <tr>
                                    <td style="text-align:center">
                                        <div id="divPaginacionVisor" style="display:none">
                                            <nav style="text-align:center">
                                                <ul class="paginacion">
                                                    <li id="liPaginaVisor"></li>
                                                </ul>
                                            </nav>
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                        
                        <br />
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>

