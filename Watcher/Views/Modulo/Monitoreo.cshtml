<div class="modal fade" id="myModalEmergencia" tabindex="-1" role="dialog" aria-labelledby="myConsultaEmergencialLabel" style="width:80%; left:10%; outline:0">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color: red">
                <button type="button" style="color:white" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myConsultaEmergencialLabel" style="font-size:14px;color:white;font-weight:bold"></h4>
            </div>
            <div class="modal-body">
                <div>
                    <table style="width:100%">
                        <tr>
                            <td>
                                <table id="tbCabecera" style="width: 100%; font-size: x-small;border:none;border-collapse:collapse"></table>
                                <div style="height:150px; overflow:auto;">
                                    <table id="tbRegConsultaEmergencia" style="width: 100%; font-size: x-small;border:none;border-collapse:collapse"></table>
                                </div>
                                <table id="tbMantenimiento">
